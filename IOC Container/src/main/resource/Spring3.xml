<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

      <bean id="employee" class="com.IOC.ConstructorInjectionBeanfactory.Employee">
          <constructor-arg name="id" value="721"></constructor-arg>
          <constructor-arg name="name" value="salina"></constructor-arg>
          <constructor-arg name="city" value="Mumbai"></constructor-arg>
     </bean>
</beans>